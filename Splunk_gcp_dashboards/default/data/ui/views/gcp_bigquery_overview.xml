<!-- v1.1-->
<form version="1.1">
  <label>BigQuery Overview</label>
  
  <search id="source_data_query">
    <query>
      |makeresults | eval addon="data."| eval dataflow="" 
      |eval datatag=`datatag`
    </query>
    <done>
      <set token="datatag">$result.datatag$</set>
    </done>
  </search>
  <search id="tstats_query">
    <query>
      |makeresults 
      |eval usetstats="|tstats fillnull_value=\"-\" count where `gcp_index` sourcetype=google:gcp:pubsub:message $datatag$protoPayload.serviceName=\"bigquery.googleapis.com\" by _time, $datatag$resource.labels.project_id, $datatag$protoPayload.methodName, $datatag$severity, $datatag$protoPayload.requestMetadata.callerIp, $datatag$protoPayload.serviceData.jobCompletedEvent.job.jobConfiguration.query.query, $datatag$protoPayload.serviceData.jobCompletedEvent.job.jobStatistics.startTime, $datatag$protoPayload.serviceData.jobCompletedEvent.job.jobStatistics.endTime, $datatag$protoPayload.authenticationInfo.principalEmail, $datatag$protoPayload.status.code, $datatag$protoPayload.status.message, $datatag$protoPayload.authorizationInfo{}.granted span=1s" 
      |eval notstats=" `gcp_index` sourcetype=google:gcp:pubsub:message $datatag$protoPayload.serviceName=\"bigquery.googleapis.com\" "
      |eval tstatstag=`tstatstag`
    </query>
    <done>
      <set token="search_tag">$result.tstatstag$</set>
    </done>
  </search>
  <search id="base_bq">
    <query> $search_tag$
      <!--|tstats fillnull_value="-" count where `gcp_index` sourcetype=google:gcp:pubsub:message $datatag$protoPayload.serviceName="bigquery.googleapis.com" by _time, $datatag$resource.labels.project_id, $datatag$protoPayload.methodName, $datatag$severity, $datatag$protoPayload.requestMetadata.callerIp, $datatag$protoPayload.serviceData.jobCompletedEvent.job.jobConfiguration.query.query, $datatag$protoPayload.serviceData.jobCompletedEvent.job.jobStatistics.startTime, $datatag$protoPayload.serviceData.jobCompletedEvent.job.jobStatistics.endTime, $datatag$protoPayload.authenticationInfo.principalEmail, $datatag$protoPayload.status.code, $datatag$protoPayload.status.message, $datatag$protoPayload.authorizationInfo{}.granted span=1s -->
    </query>
    <earliest>$time.earliest$</earliest>
    <latest>$time.latest$</latest>
  </search>
  <fieldset submitButton="false">
    <input type="time" token="time">
      <label></label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="dropdown" token="project_tok">
      <label>Project ID</label>
      <fieldForLabel>ProjectID</fieldForLabel>
      <fieldForValue>ProjectID</fieldForValue>
      <search base="base_bq">
        <query>|rename $datatag$resource.labels.project_id as ProjectID | stats count by ProjectID </query>
      </search>
      <default>*</default>
      <initialValue>*</initialValue>
      <choice value="*">ALL</choice>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Request location</title>
      <map>
        <search base="base_bq">
          <query>
            <!--`gcp_index` sourcetype=google:gcp:pubsub:message bigquery $datatag$protoPayload.serviceName="bigquery.googleapis.com" | iplocation $datatag$protoPayload.requestMetadata.callerIp | geostats latfield=lat longfield=lon count by $datatag$protoPayload.requestMetadata.callerIp
            -->
              search $datatag$resource.labels.project_id =$project_tok$ $datatag$protoPayload.requestMetadata.callerIp!="-" | iplocation $datatag$protoPayload.requestMetadata.callerIp | geostats latfield=lat longfield=lon count by $datatag$protoPayload.requestMetadata.callerIp
