<!-- v1.1-->
<form version="1.1">

  <label>Cloud SQL Overview</label>

  <description>V1.0</description>
  
  <search id="source_data_query">
    <query>
      |makeresults | eval addon="data."| eval dataflow="" 
      |eval datatag=`datatag`
    </query>
    <done>
      <set token="datatag">$result.datatag$</set>
    </done>
  </search>
  <search id="tstats_query">
    <query>
      |makeresults 
      |eval usetstats="|tstats fillnull_value=\"-\" count where `gcp_index` sourcetype=google:gcp:pubsub:message OR sourcetype=google:gcp:pubsub:audit:auth $datatag$protoPayload.methodName=\"*iam*\" by _time, $datatag$resource.type, $datatag$protoPayload.methodName, $datatag$resource.labels.project_id, $datatag$protoPayload.authenticationInfo.principalEmail, $datatag$protoPayload.authorizationInfo{}.permission, $datatag$protoPayload.serviceData.policyDelta.bindingDeltas{}.action, $datatag$protoPayload.serviceData.policyDelta.bindingDeltas{}.role span=1s" 
      |eval notstats="`gcp_index` sourcetype=google:gcp:pubsub:message $datatag$protoPayload.serviceName=\"iam.googleapis.com\""
      |eval searchtag=`tstatstag`
    </query>
    <done>
      <set token="tstatstag">$result.tstatstag$</set>
      <set token="tstats_post_midtag">$result.tstats_post_midtag$</set>
      <set token="tstats_post_endtag">$result.tstats_post_endtag$</set>
    </done>
  </search>
  <search id="base_sql">
    <query>
        <!--|tstats fillnull_value="-" count where `gcp_index` sourcetype=google:gcp:pubsub:message $datatag$protoPayload.serviceName="iam.googleapis.com" by _time, $datatag$protoPayload.methodName, $datatag$resource.labels.project_id, $datatag$protoPayload.authenticationInfo.principalEmail, $datatag$protoPayload.authorizationInfo{}.permission, $datatag$protoPayload.serviceData.policyDelta.bindingDeltas{}.action, $datatag$protoPayload.serviceData.policyDelta.bindingDeltas{}.role -->
        <!--$searchtag$-->
        | tstats fillnull_value="-" count where `gcp_index` sourcetype=google:gcp:pubsub:message data.protoPayload.serviceName="cloudsql.googleapis.com" by _time, $datatag$resource.labels.project_id, $datatag$protoPayload.requestMetadata.callerIp, $datatag$protoPayload.methodName, $datatag$protoPayload.resourceName, $datatag$resource.labels.region, $datatag$protoPayload.authenticationInfo.principalEmail, $datatag$severity span=1s
    </query>
    <earliest>$time.earliest$</earliest>
    <latest>$time.latest$</latest>
  </search>
  
  <fieldset submitButton="false">
    <input type="time" token="time">
      <label>Time</label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
