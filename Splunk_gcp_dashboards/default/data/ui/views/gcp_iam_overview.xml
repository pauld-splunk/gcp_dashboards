<!-- v2.7-->
<!-- Add-On 4.0 -->
<form version="1.1">
  <label>IAM Overview</label>
  <search id="source_data_query">
    <query>
      |makeresults | eval addon="data."| eval dataflow="" 
      |eval datatag=`datatag`
    </query>
    <done>
      <set token="datatag">$result.datatag$</set>
    </done>
  </search>
  <search id="tstats_query">
    <query>
      |makeresults 
      |eval usetstats="|tstats fillnull_value=\"-\" count where `gcp_index` `gcp_sourcetype_iam_activity` $datatag$protoPayload.methodName=\"*iam*\" by _time, $datatag$resource.type, $datatag$protoPayload.methodName, $datatag$resource.labels.project_id, $datatag$protoPayload.authenticationInfo.principalEmail, $datatag$protoPayload.authorizationInfo{}.permission, $datatag$protoPayload.serviceData.policyDelta.bindingDeltas{}.action, $datatag$protoPayload.serviceData.policyDelta.bindingDeltas{}.role span=1s" 
      |eval notstats="`gcp_index` `gcp_sourcetype_iam_activity` $datatag$protoPayload.serviceName=\"*iam*\" |fields _time, $datatag$resource.type, $datatag$protoPayload.methodName, $datatag$resource.labels.project_id, $datatag$protoPayload.authenticationInfo.principalEmail, $datatag$protoPayload.authorizationInfo{}.permission, $datatag$protoPayload.serviceData.policyDelta.bindingDeltas{}.action, $datatag$protoPayload.serviceData.policyDelta.bindingDeltas{}.role"
      |eval tstatstag=`tstatstag`
    </query>
    <done>
      <set token="tstatstag">$result.tstatstag$</set>
      <set token="tstats_post_midtag">$result.tstats_post_midtag$</set>
      <set token="tstats_post_endtag">$result.tstats_post_endtag$</set>
    </done>
  </search>
  <search id="base_iam">
    <query>
        $tstatstag$
    </query>
    <earliest>$time.earliest$</earliest>
    <latest>$time.latest$</latest>
  </search>
  <fieldset submitButton="false">
    <input type="time" token="time">
      <label>Time</label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="dropdown" token="project_tok" searchWhenChanged="true">
      <label>Project</label>
      <choice value="*">ALL</choice>
      <fieldForLabel>ProjectID</fieldForLabel>
      <fieldForValue>ProjectID</fieldForValue>
      <search  base="base_iam">
        <query> rename $datatag$resource.labels.project_id as ProjectID | stats count by ProjectID </query>
      </search>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Operations</title>
      <chart>
        <search base="base_iam">
          <query> search $datatag$resource.labels.project_id =$project_tok$ | timechart count by $datatag$protoPayload.methodName
          </query>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
