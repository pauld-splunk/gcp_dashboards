<form>
  <search id="baseInfo">
    <query>
      | mstats max(_value) as Avg  WHERE metric_name="cloudfunctions.googleapis.com/function/execution_times" AND index="gcp_metrics"  span=1h earliest=-24h BY function_name, project_id,region, memory,status, trigger_metric_name
| dedup function_name,project_id
    </query>
  </search>
  <label>GCP Cloud Functions</label>
  <fieldset submitButton="false">
    <input type="time" token="field1">
      <label></label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="multiselect" token="project_id_tok" searchWhenChanged="true">
      <label>Project ID</label>
      <delimiter> ,</delimiter>
      <fieldForLabel>project_id</fieldForLabel>
      <fieldForValue>project_id</fieldForValue>
      <search base="baseInfo">
        <query>dedup project_id</query>
      </search>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="multiselect" token="zone_tok" searchWhenChanged="true">
      <label>Zone</label>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
      <delimiter>,</delimiter>
      <fieldForLabel>region</fieldForLabel>
      <fieldForValue>region</fieldForValue>
      <search base="baseInfo">
        <query>search project_id IN ($project_id_tok$)|dedup region</query>
      </search>
    </input>
    <input type="multiselect" token="function_tok" searchWhenChanged="true">
      <label>Function</label>
      <delimiter> ,</delimiter>
      <fieldForLabel>function_name</fieldForLabel>
      <fieldForValue>function_name</fieldForValue>
      <search base="baseInfo">
        <query>search project_id IN ($project_id_tok$) AND region IN ($zone_tok$) |dedup function_name</query>
      </search>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Functions</title>
      <table>
        <search base="baseInfo">
          <query>search project_id IN ($project_id_tok$) AND region IN ($zone_tok$) AND function_name IN ($function_tok$)
| rename function_name as Function, project_id as Project,region as Zone, status as Status, memory as Memory, trigger_metric_name as Trigger
|table Project, Zone, Function, Memory, Status, Trigger </query>
        </search>
        <!--  <link target="_blank"></link>
          project=$click.name$&amp;clickvalue=$click.value$&amp;function=$row.Function$
           /app/gcp_dashboards/cloud_function_details?$project$=$click.name$&amp;$clickvalue$=$click.value$&amp;$function$=$row.Function$ -->
        <option name="count">20</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
        <drilldown>
          <set token="cfunction">$row.Function$</set>
          <set token="cproject">$row.Project$</set>
          <link target="_blank">/app/gcp_dashboards/cloud_function_details?form.project_id_tok=$cproject$&amp;form.function_tok=$cfunction$</link>
        </drilldown>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <html>
        <center>
          <h1>Click on a Function above to get more details</h1>
        </center>
      </html>
    </panel>
  </row>
  <row>
    <panel>
